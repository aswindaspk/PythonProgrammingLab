   ],
   "source": [
    "n=int(input('Enter a number: '))\n",
    "arm=0\n",
    "num=n\n",
    "while(n>0):\n",
    "    d=n%10\n",
    "    arm=arm+d*d*d\n",
    "    n=n//10\n",
    "if(arm==num):\n",
    "    print(num,'is an Armstrong number')\n",
    "else:\n",
    "    print(num,'is not an Armstrong number')"
   ]
