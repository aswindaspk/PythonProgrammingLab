    "#gcd of 2 numbers\n",
    "n1=int(input('Enter first number: '))\n",
    "n2=int(input('Enter second number: '))\n",
    "j=min(n1,n2)\n",
    "i=j-1\n",
    "while(i>0):\n",
    "    if(n2%i==0 and n1%i==0):\n",
    "        print('GCD is',i)\n",
    "        break\n",
    "    i=i-1"
