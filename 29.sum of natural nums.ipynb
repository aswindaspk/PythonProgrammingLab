   "source": [
    "#sum of natural numbers upto n\n",
    "n=int(input('Enter the range: '))\n",
    "i=1\n",
    "sum=0\n",
    "while(i<=n):\n",
    "    sum=sum+i\n",
    "    i=i+1\n",
    "print('Sum of %d natural numbers are %d' %(n,sum))"
